@model Expenses_Tracker.Models.AuthenticationModels.UserAuthenticationModel
@{
    ViewData["Title"] = "SignIn";
}

<h1>SignIn</h1>

<form method="post">
    <input asp-for="Email" type="text" placeholder="Username...">
    <input asp-for="Password" type="text" placeholder="Password...">
    <button type="submit" value="Submit">Login</button>
</form>
